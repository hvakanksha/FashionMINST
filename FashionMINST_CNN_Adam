      ],
      "source": [
        "model = model_arch()\n",
        " \n",
        "model.compile(optimizer=Adam(learning_rate=1e-3),\n",
        "              loss='sparse_categorical_crossentropy',\n",
        "              metrics=['sparse_categorical_accuracy'])\n",
        " \n",
        "model.summary()"
      ]
